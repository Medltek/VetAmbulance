@model IEnumerable<VetAmbulance.BL.Models.Diagnosis>

@{
    ViewData["Title"] = "Diagnoses";
}

<h1>Your Diagnosis</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Date
            </th>
            <th>
                Disease
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DisName)
                </td>
                <td>
                    @Html.ActionLink("Details", "Detail", new { id = item.Id })
                </td>
            </tr>
        }
    </tbody>
</table>
